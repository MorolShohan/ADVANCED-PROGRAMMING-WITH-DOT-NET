
@{
    ViewBag.Title = "Index";
}



<h2>Index</h2>
<form method="post" action="">

    <label for="name">Name:</label>
    <input type="text" name="name" value="@Model.name" class="form-check">
    <span class="text-danger">@Html.ValidationMessage("name") </span>

    <br>

    <label for="Index">Index:</label>
    <input type="text" name="index" value="@Model.index" placeholder="xx-xxxxx-x" class="form-check">
    <span class="text-danger">@Html.ValidationMessage("index") </span>
    <br>

    <label for="email">Email:</label>
    <input type="email" name="email" value="@Model.email" class="form-check">
    <span class="text-danger">@Html.ValidationMessage("email") </span>

    <br>

    <input type="submit" value="submit">
</form>

